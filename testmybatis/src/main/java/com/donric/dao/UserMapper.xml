<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--绑定一个mapper接口-->
<mapper namespace="com.donric.dao.UserDao">
<!--    resultType返回一个，resultMap返回多个-->
    <select id="getUserList" resultType="com.donric.pojo.User">
    select * from mybatis.user
  </select>
</mapper>